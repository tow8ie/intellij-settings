<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'Local REPL'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'Local REPL'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Evaluate Expression...">
        <Item>
          <IdeaActionID>EvaluateExpression</IdeaActionID>
          <ShortCut>'⌥F8'</ShortCut>
          <Description>Evaluate Expression...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Favorites</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>25</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Local REPL'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'Local REPL'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Remote REPL'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'Remote REPL'</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'pytest for cdcs_test.test_combopromo_cdc'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'pytest for cdcs_test.test_combopromo_cdc'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Execution Point">
        <Item>
          <IdeaActionID>ShowExecutionPoint</IdeaActionID>
          <ShortCut>'⌥F10'</ShortCut>
          <Description>Show Execution Point</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>25</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Local REPL'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Local REPL'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Test">
        <Item>
          <IdeaActionID>GotoTest</IdeaActionID>
          <ShortCut>'⇧⌘T'</ShortCut>
          <Description>Test</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>