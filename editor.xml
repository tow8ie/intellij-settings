    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo\b:\s\[527\].*" />
    <filter name="FTNAV-527">
      <pattern index="2" />
    </filter>
  </component>
