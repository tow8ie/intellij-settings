<templateSet group="Clojure">
  <template name="showgraph" value="(ugraph/viz-graph $graph$ {:auto-label true})" description="Visualizes an ubergraph" toReformat="false" toShortenFQNames="true">
    <variable name="graph" expression="complete()" defaultValue="graph" alwaysStopAt="true" />
    <context>
      <option name="Clojure" value="true" />
    </context>
  </template>
  <template name="wat" value="(is (= :what?&#10;       $expr$))" shortcut="ENTER" description="Skeleton for a test assertion that checks for :what?" toReformat="false" toShortenFQNames="true">
    <variable name="expr" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Clojure" value="true" />
    </context>
  </template>
</templateSet>